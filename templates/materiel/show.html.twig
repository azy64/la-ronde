    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ materiel.id }}</td>
            </tr>
            <tr>
                <th>Keys</th>
                <td>{{ materiel.keys ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Radio</th>
