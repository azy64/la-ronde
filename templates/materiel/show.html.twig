            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ materiel.phone ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Round_controller</th>
                <td>{{ materiel.roundController ? 'Yes' : 'No' }}</td>
